<!DOCTYPE html>
<html>
<head>
  <title>Upload Result</title>
  <link rel="stylesheet" href="./css/uploadResult.css">
</head>
<body>
  <h1>Upload Summary</h1>

  <p><strong>Inserted:</strong> <%= inserted %></p>

  <% if (errors.length > 0) { %>
    <h3>Errors:</h3>
    <ul>
      <% errors.forEach(function(err) { %>
        <li><%= err %></li>
      <% }) %>
    </ul>
  <% } else { %>
    <p>No errors ðŸŽ‰</p>
  <% } %>
</body>
</html>
